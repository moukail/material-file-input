{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@moukail/material-file-input",
  "version": "9.0.0",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Jeremy Legros"
  },
  "homepage": "https://merlosy.github.io/ngx-material-file-input/",
  "keywords": [
    "angular",
    "material",
    "file",
    "input",
    "mat-form-field"
  ],
  "bugs": {
    "url": "https://github.com/merlosy/ngx-material-file-input/issues"
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "lint": "tslint 'src/**/*.ts' --fix",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/material-file-input && npm publish --access public && cd ../../"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moukail/ngx-material-file-input"
  },
  "peerDependencies": {
    "@angular/cdk": "^9.0.0-rc.10",
    "@angular/common": "^9.0.0-rc.14",
    "@angular/core": "^9.0.0-rc.14",
    "@angular/material": "^9.0.0-rc.10"
  },
  "devDependencies": {
    "@angular/core": "^9.0.0-rc.11",
    "@angular/animations": "^9.0.0-rc.11",
    "@angular/common": "^9.0.0-rc.11",
    "@angular/platform-browser": "^9.0.0-rc.11",
    "@angular/forms": "^9.0.0-rc.11",
    "@angular/material": "^9.0.0-rc.8",
    "@angular/cdk": "^9.0.0-rc.8",
    "@angular/compiler": "^9.0.0-rc.11",
    "@angular/compiler-cli": "^9.0.0-rc.11",
    "ng-packagr": "5.7.1",
    "rimraf": "3.0.1",
    "typescript": "~3.6.4"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "dest": "./dist/material-file-input"
  }
}
